<%- include('./engine/header'); %>

        <!-- Page Content-->
        <section class="pt-4">
            <div class="container px-lg-5">
                <!-- Page Features-->
            </br>
                <div class="row gx-lg-5">
                    <div class="col-lg-12">
                        <table id="example">
                            <thead>
                                <tr>
                                    <th>Nama Barang</th>
                                    <th>Quantity</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                    <% if(data_barang){ %>
                                        <% data_barang.forEach(function(data) { %>
                                            <tr>
                                                <td><%= data.name %></td>
                                                <td><%= data.qty %></td>
                                                <td><a href="/add/barang/admin/<%= data.id %>" class="btn btn-primary">Update</a></td>
                                            </tr>
                                        <% }); %>
                                    <% } else{ %>
                                    <% } %>
                            </tbody>
                        </table>
                    </div>

                </div>
            </div>
        </section>

<%- include('./engine/footer'); %>